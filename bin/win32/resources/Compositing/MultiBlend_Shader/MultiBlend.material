fragment_program MultiBlend/Blend cg
{
    source MultiBlend.cg
    entry_point MultiBlend_ps
    profiles fp40
}

// scene pass
material MultiBlend/ScenePass1
{
	technique
	{
		pass
		{
			cull_hardware none
			cull_software none
			depth_check off
			depth_write off

			fragment_program_ref CompositorTemplate/SceneFP
			{
			}
			texture_unit
			{
				tex_coord_set 0
				tex_address_mode clamp
				filtering none
			}
		}
	}
}

