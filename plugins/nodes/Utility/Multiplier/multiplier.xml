<!DOCTYPE nodetype SYSTEM "../../../nodetype.dtd">
<nodetype name="Multiplier" category="Utility" color="255, 192, 171" plugin="multiplier.dll">
  <parameters>
	<parameter name="multi" type="Float" defaultValue="1.0" minValue="-1000" maxValue="1000" pin="in" selfEvaluating="true" />
  <parameter name="input" type="Float" defaultValue="0.0" minValue="-1000" maxValue="1000" pin="in" selfEvaluating="true"/>
	<parameter name="output" type="Float" defaultValue="0.0" minValue="-1000" maxValue="1000" pin="out" selfEvaluating="true"/>
  </parameters>
</nodetype>
