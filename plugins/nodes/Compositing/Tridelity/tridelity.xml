<!DOCTYPE nodetype SYSTEM "../../../nodetype.dtd">
<nodetype name="Tridelity" category="Compositing" color="185, 30, 75" plugin="tridelity.dll">
  <parameters>
    <parameter name="Input Map 0" type="Image" pin="in" />
    <parameter name="Input Map 1" type="Image" pin="in" />
    <parameter name="Input Map 2" type="Image" pin="in" />
    <parameter name="Input Map 3" type="Image" pin="in" />
    <parameter name="Input Map 4" type="Image" pin="in" />
    
    <parameter name="Resolution X" type="Int" defaultValue="0" minValue="0" maxValue="4096"/>
    <parameter name="Resolution Y" type="Int" defaultValue="0" minValue="0" maxValue="4096"/>

    <parameter name="Screen Type" type="Enumeration" defaultValue="ML">
      <literal name="ML" value="ML"/>
      <literal name="MV" value="MV"/>
    </parameter>
  </parameters>
</nodetype>


